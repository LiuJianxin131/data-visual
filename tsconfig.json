{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es7"
    ],
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@base/*": ["./FrontEnd/base/*"],
      "@pages/*": ["./FrontEnd/pages/*"],
      "@components/*": ["./FrontEnd/components/*"],
      "@charts/*": ["./FrontEnd/charts/*"],
      "@lib/*": ["./FrontEnd/lib/*"]
    }
  }
}