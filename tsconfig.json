{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "target": "es6",
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es7"
    ],
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@base/*": ["./FrontEnd/base/*"],
      "@pages/*": ["./FrontEnd/pages/*"],
      "@components/*": ["./FrontEnd/components/*"],
      "@container/*": ["./FrontEnd/container/*"],
      "@lib/*": ["./FrontEnd/lib/*"],
      "@hoc/*": ["./FrontEnd/hoc/*"],
      "@controls/*": ["./FrontEnd/controls/*"],
      "@charts": ["./FrontEnd/charts/index.ts"],
      "@assets": ["./FrontEnd/assets/*"]
    }
  }
}